{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",

    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    "skipLibCheck": true,
    "resolveJsonModule": true,

    "types": ["node"]
  },
  "include": ["src", "../crawler-service/src/parsers/uiPattern.parser.ts"],
  "exclude": ["node_modules"]
}
