{
  "manifest_version": 3,
  "name": "StackLens",
  "version": "1.0",
  "description": "Analyze any website instantly using StackLens.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": [
    "http://localhost:5000/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "StackLens"
  }
}